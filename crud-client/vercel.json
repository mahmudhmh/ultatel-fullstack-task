{
  "version": 2,
  "builds": [
    {
      "src": "dist/crud-client/browser/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/home",
      "dest": "/home/index.html"
    },
    {
      "src": "/login",
      "dest": "/login/index.html"
    },
    {
      "src": "/register",
      "dest": "/register/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
